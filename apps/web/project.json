{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "root": "apps/web",
  "sourceRoot": "apps/web/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project web --web",
  "targets": {
    "build": {
      "cache": true,
      "inputs": [
        {
          "externalDependencies": ["@react-router/dev"]
        }
      ],
      "outputs": ["{workspaceRoot}/dist/apps/web"],
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "cwd": "apps/web",
          "command": "react-router build --minify=false"
        },
        "production": {
          "cwd": "apps/web",
          "command": "react-router build --minify=true --sourcemapClient=false --sourcemapServer==false"
        }
      },
      "parallelism": true
    },
    "lint": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "command": "eslint ."
      },
      "inputs": ["default", "^default"],
      "outputs": ["{options.outputFile}"],
      "configurations": {},
      "parallelism": true
    },
    "typegen": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "command": "react-router typegen"
      }
    },
    "// targets": {}
  }
}
